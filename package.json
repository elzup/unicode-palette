{
  "name": "unicode-palette",
  "version": "1.0.0",
  "description": "Unicode character visualization tool - Generate grid-based character maps as SVG/PNG",
  "author": "elzup",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "unicode-palette": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs"
    }
  },
  "repository": "elzup/unicode-palette",
  "bugs": {
    "url": "https://github.com/elzup/unicode-palette/issues"
  },
  "homepage": "https://github.com/elzup/unicode-palette#readme",
  "keywords": [
    "unicode",
    "svg",
    "png",
    "character",
    "visualization",
    "grid"
  ],
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "build": "unbuild",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build && pnpm test",
    "generate:data": "tsx scripts/generate-unicode-data.ts",
    "render:svg": "tsx src/generator/render-sample-svg.ts",
    "render:png": "tsx src/generator/render-sample.ts",
    "render:tiles": "tsx src/generator/render-script-tiles.ts"
  },
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@elzup/tsconfig": "^1.1.0",
    "@types/node": "^22.0.0",
    "@unicode/unicode-17.0.0": "^1.6.16",
    "canvas": "^3.2.0",
    "oxlint": "^1.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "unbuild": "^3.6.1",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "canvas": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "canvas": {
      "optional": true
    }
  },
  "dependencies": {
    "citty": "^0.1.6",
    "zod": "^4.1.13"
  }
}
