{
  "name": "unicode-palette",
  "version": "1.0.3",
  "description": "Unicode character data and utilities",
  "author": "elzup",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "unicode-palette": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": "elzup/unicode-palette",
  "bugs": {
    "url": "https://github.com/elzup/unicode-palette/issues"
  },
  "homepage": "https://github.com/elzup/unicode-palette#readme",
  "keywords": [
    "unicode",
    "character",
    "codepoint",
    "block"
  ],
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.30.3",
  "scripts": {
    "build": "unbuild",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build && pnpm test",
    "generate:data": "tsx scripts/generate-unicode-data.ts"
  },
  "files": [
    "dist/",
    "data/unicode-blocks.min.json"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@elzup/tsconfig": "^1.1.0",
    "@types/node": "^24.0.0",
    "@unicode/unicode-17.0.0": "^1.6.16",
    "oxlint": "^1.0.0",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "unbuild": "^3.6.1",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "citty": "^0.2.0",
    "zod": "^4.1.13"
  }
}
